		{
			"character": "character-1690799344.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Hi! My name is Gogy!!!\nI love new guests!!! What brought you here?"
		},
		{
			"character": "character-1690725880.json",
			"event_id": "dialogic_001",
